{
  "name": "sosmedcrawling",
  "version": "1.0.0",
  "description": "Crawling data sosial media menggunakan playwright",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mudaberani/sosmedcrawling.git"
  },
  "keywords": [
    "crawling",
    "sosial media",
    "playwright",
    "instagram",
    "tiktok",
    "youtube"
  ],
  "author": "Dika Haris Abdurahman <dika.webmaster@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^10.0.0",
    "papaparse": "^5.4.1",
    "playwright": "^1.33.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@types/papaparse": "^5.3.7",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "jest-environment-node": "^27.5.1",
    "jest-playwright-preset": "^1.7.0",
    "jest-runner": "^27.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "preset": "jest-playwright-preset"
  },
  "tsconfig": {
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
}
